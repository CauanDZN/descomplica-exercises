Algoritmo "TipoDadosDiferentes"

Var
    nome: Caracter
    idade: Inteiro
    altura: Real
    crianca: Logico

Inicio
    nome <- "Cauan"
    idade <- 18
    altura <- 1.80
    crianca <- falso

    Escreval("Tipo de Caracter: ", nome)
    Escreval("Tipo de Inteiro: ", idade)
    Escreval("Tipo de Real: ", altura)
    Escreval("Tipo de Booleano: ", crianca)

Fimalgoritmo
